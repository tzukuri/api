<h1>Devices</h1>
<ul>
    <% @auth_tokens.each do |token| %>
        <li><a href="/diagnostics/<%= token.diagnostics_sync_token %>"><%= token.user.name %> - <%= token.api_device.name %></a></li>
    <% end %>
</ul>

<br/><br/>

<h1>No Auth Tokens</h1>
<ul>
    <% @no_auth_tokens.each do |token| %>
      <li><a href="/diagnostics/<%= token %>"><%= token %></a></li>
    <% end %>
</ul>
